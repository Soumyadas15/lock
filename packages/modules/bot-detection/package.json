{
  "name": "@lock-sdk/bot-detection",
  "version": "1.0.0",
  "description": "Bot detection module for Lock security framework",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @lock-sdk/sdk/core",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest"
  },
  "keywords": [
    "security",
    "bot-detection",
    "lock",
    "web-security"
  ],
  "author": "Lock team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@lock-sdk/core": "^1.0.0",
    "@upstash/redis": "^1.34.6",
    "axios": "^1.8.4",
    "lru-cache": "^11.1.0",
    "redis": "^4.7.0"
  },
  "peerDependencies": {
    "@lock-sdk/core": "^1.0.0"
  },
  "devDependencies": {
    "@lock-sdk/eslint-config": "workspace:*",
    "@lock-sdk/typescript-config": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "jest": "^29.7.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
